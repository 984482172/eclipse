curl -L http://password.corp.redhat.com/RH-IT-Root-CA.crt -o /etc/pki/ca-trust/source/anchors/RH-IT-Root-CA.crt
update-ca-trust enable
update-ca-trust extract
yum install -y git 
git clone https://gitlab.cee.redhat.com/kvm-qe/kar.git && cd kar && ./Bootstrap.sh --develop --venv --upstream --verbose
